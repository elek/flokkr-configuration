yarn.resourcemanager.hostname: [[ service "resourcemanager"]]
yarn.nodemanager.pmem-check-enabled: false
yarn.nodemanager.delete.debug-delay-sec: 600
yarn.nodemanager.vmem-check-enabled: false
yarn.nodemanager.aux-services: mapreduce_shuffle
yarn.nodemanager.delete.thread-count: 0
yarn.nodemanager.local-dirs: /data/nodemanagerlocaldir
#yarn.nodemanager.log-dirs: /data/yarn-userlogs
#yarn.nodemanager.remote-app-log-dir: /data/yarn-remotelogs
yarn.resourcemanager.bind-host: 0.0.0.0
yarn.nodemanager.bind-host: 0.0.0.0
yarn.scheduler.maximum-allocation-mb: 12000
yarn.nodemanager.resource.memory-mb: 12000
